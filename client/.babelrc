{
  "presets": [
    [
      "@babel/preset-env",
      {
        "debug": true,
        "useBuiltIns": "entry",
        "corejs": "core-js@2"
      }
    ],
    "@babel/preset-react",
    "airbnb"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": [
          "./"
        ],
        "alias": {
          "@components": "./src/components"
        }
      }
    ],
    "@babel/plugin-proposal-class-properties"
  ]
}