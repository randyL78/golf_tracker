{
  "name": "golf_tracker",
  "version": "1.0.0",
  "description": "Express and React App for keeping track of how well you did in a round of golf. Additionally, uses weather API to warn of stormy weather approaching while out on the links and Giphy API for displaying gif's for great shot's, etc.",
  "main": "server/app.js",
  "dependencies": {
    "basic-auth": "^2.0.1",
    "body-parser": "^1.20.1",
    "concurrently": "^4.1.1",
    "core-js": "^2.6.9",
    "express": "^4.18.2",
    "express-pino-logger": "^4.0.0",
    "http-errors": "^1.7.2",
    "mongodb": "^3.2.7",
    "mongoose": "^5.5.9",
    "morgan": "^1.9.1",
    "nodemon": "^1.19.0",
    "pino-colada": "^1.4.4",
    "shortid": "^2.2.14",
    "slug": "^1.1.0",
    "uuidv4": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/node": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.11.1"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start-server": "babel-node app.js | pino-colada",
    "start-client": "cd ../client && npm start",
    "start": "concurrently \"npm run start-server\" \"npm run start-client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/randyL78/golf_tracker.git"
  },
